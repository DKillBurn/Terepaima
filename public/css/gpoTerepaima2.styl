	float right
	label
		font-family 'TrebuchetN'
		font-size 14px
		text-align left
		color azul_darken
		display block

header
	background url('../images/header-bg1.png') center top no-repeat
	background-position-y 100%
	//min-height 477px
	//background-size cover
	height 540px
	overflow hidden
	.logotipo
		background url('../images/row-top-tail.png') center 13px repeat-x
		text-align center
		display block
		width 100%
		height 150px
		//padding 1.5em 2% 1.5em 10%
		.logo
			//border 1px dashed invert(azul)
			//width 520px
			//height 90px
			background url('../images/logoWeb.png') no-repeat
			text-indent -5000px
			width 390px
			height 67px
			margin-top 45px
			display block
			float left
		nav
			background-color azul_light
			display block
			.menu
				//padding 
				//text-align center
				margin-top 4.4em
				vertical-align middle
				white-space nowrap
				float right
				li
					//font-size 17px
					display inline-block
					a
						margin-left 2em
						padding 0.5em
						color azul
						text-decoration none
						transition 0.5s
					a:hover
						margin-left 2em
						padding 0.5em
						background-color azul_darken
						color azul_lighter
						border-radius 10px
						box-shadow 0 0 5px black inset
						text-decoration none
						transition 0.5s
					a:active
						margin-left 2em
						padding 0.5em
						background-color azul_darken
						color azul_lighter
						border-radius 10px
						box-shadow 0 0 5px black inset
						text-decoration none
						transition 0.5s
	.eslogan
		padding-top 15px
		margin 30px auto
		text-align center
		vertical-align middle
		//background-color azul_darken

.art-text
	font-size 1em
	font-family Helvetica
	
.more
	text-align left
.more a
	padding 0.5em 1em
	background-color azul_light
	color azul_darken
	border-radius 10px
	text-decoration none
	transition 0.5s
.more a:hover
	padding 0.5em 1em
	background-color azul_darken
	color azul_lighter
	border-radius 10px
	box-shadow 0 0 5px black inset
	text-decoration none
	transition 0.5s
.autor
	text-align right
	a
		display inline-block

body
	background-color azul_darken
	section
		background-color azul_lighter
		border 1px solid azul_dark
		.servicio
			min-height 120px
			padding 20px 0
			text-align center
			background url('../images/bg-tail-rotated.png') left bottom repeat-x gris
			display block
			.srv-init
				padding 12px 32px 20px 0
				margin-left 1em
				width 250px
				text-align center
				background url('../images/sep.gif') right top repeat-y
				display inline-block
				float left
				a
					margin-bottom 20px
					width 84px
					height 88px
					background url('../images/logistics-img.png') no-repeat
					text-indent -5000px
					display inline-block
			.srv-med
				width 250px
				padding 12px 16px
				text-align center
				background url('../images/sep.gif') right top repeat-y
				display inline-block
				float left
				a
					margin-bottom 20px
					width 83px
					height 87px
					background url('../images/entertainment-img.png') no-repeat
					text-indent -5000px
					display inline-block
			.srv-last
				width 250px
				padding 12px 0 20px 32px
				margin-right 1em
				text-align center
				//background url('../images/sep.gif') right top repeat-y
				display inline-block
				float left
				a
					margin-bottom 20px
					width 83px
					height 86px
					background url('../images/tech-img.png') no-repeat
					text-indent -5000px
					display inline-block
		.noticia
			margin 3em 0
			text-align center
			vertical-align middle
			min-height 500px
			.post
				//background linear-gradient(azul_dark, azul_darken 10%, azul_darken 90%, azul_dark)
				margin-left 20px
				margin-right 60px
				width 500px
				height 250px
				float left
				overflow hidden
				.titular
					width 100%
					text-align initial
					color azul_darken
					white-space normal
					display block
				.subtitular
					width 100%
					text-align initial
					color azul
				.detalles
					width 100%
					margin-bottom 1em
					text-align justify
					white-space normal
					overflow hidden
			.posts
				//background linear-gradient(azul_dark, azul_darken 10%, azul_darken 90%, azul_dark)
				display inline-block
				width 300px
				overflow hidden
				.masnoticias
					text-align left
					padding-bottom 0.5em
					margin-bottom 0.5em
					border-bottom thin solid azul_darken
				.minipost
					display block
					height 100px
					margin-bottom 0.5em
					.minimagen 
						margin-right 0.5em
						display inline-block
						float left
						img
							border 1px solid azul_darken
							width 100px
							height 100px
					.minifecha
						text-align left
						color azul
						margin
						padding 0.8em 0	
					.minidescripcion
						text-align left
						overflow hidden
						text-overflow ellipsis
						padding-top 0.8em
						p
							width 190px
							white-space normal
							-webkit-line-clamp 3
							-webkit-box-orient vertical
							display block
							display -webkit-box
							
							
						
@media screen and (min-width: 992px)

	.wrapper
		width 900px
		margin 0 auto
		overflow hidden
		
	body
		section
			.noticia
				min-height 360px
				.post
					width 500px
					height 250px
					float left
					margin-left 0
					
	header
		height 477px
		background-size auto
		.logotipo
			width 100%
			.logo
			nav
				.menu
					li
						a
							margin-left 2em
							padding 0.5em
						a:hover
							margin-left 2em
							padding 1em 0.5em
						a:active
							margin-left 2em
							padding 1em 0.5em
		.eslogan
			margin 0px 100px