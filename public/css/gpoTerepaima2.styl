			width 100%
			height 200px
			float left
			table
				margin auto
				p
					display inline-block
				textarea
					resize none
		.con_new-news
			textarea
				resize none
			
	.btn
		position absolute
		bottom 0
		right 0

.table
	table-layout fixed
	
th
	background-color azul_darken
