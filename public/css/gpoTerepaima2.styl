azul = #48BCEC
azul_dark = #23A9E3
azul_darken =  #0796D3
azul_lighter= #A1DDF8
azul_light = #6FCBF2
gris = #F8F8F8
gris_dark = #808080

@font-face {
	font-family: 'iconotes';
	src:url('../fonts/icomoon.eot?-bgakk3');
	src:url('../fonts/icomoon.eot?#iefix-bgakk3') format('embedded-opentype'),
		url('../fonts/icomoon.woff?-bgakk3') format('woff'),
		url('../fonts/icomoon.ttf?-bgakk3') format('truetype'),
		url('../fonts/icomoon.svg?-bgakk3#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}

@font-face {
    font-family: 'TrebuchetN';
    src: url('../fonts/TrebuchetN.ttf') format('truetype');
    font-weight: normal;
	font-style: normal;
}

@font-face {
    font-family: 'TrebuchetB';
    src: url('../fonts/TrebuchetB.ttf') format('truetype');
    font-weight: normal;
	font-style: normal;
}

@font-face {
    font-family: 'TrebuchetI';
    src: url('../fonts/TrebuchetI.ttf') format('truetype');
    font-weight: normal;
	font-style: normal;
}


a
	font-family 'TrebuchetN'
	color azul_darken
	text-decoration none
	
p
	font-family 'TrebuchetN'
*
	//letter-spacing 1.5px
	margin 0
	padding 0

.wrapper
	min-width 900px
	white-space nowrap
	display inline-block
	margin 0 auto
	overflow hidden

footer
	background url('../images/bg-tail.png') left top repeat-x
	padding 2em 2em
	margin 0
	text-align center

strong
	font-family 'TrebuchetB'
h2
	font-family 'TrebuchetB'
	color azul_darken
	font-size 1.5em
	white-space normal
	padding-bottom 0.5em

h3
	font-family 'TrebuchetB'
	color azul_darken
	font-size 1.5em
	
h4
	font-family 'TrebuchetI'
	color azul
	padding-bottom 1em
	
h5
	font-family 'TrebuchetB'
	color azul
	font-style italic
	padding-bottom 1em

.notification
	background-color #F7BABA
	color red
	text-align center
	height 32px
	width 100%
	display none

.h2-alt
	text-align right
	color azul_darken
	font-size 1.5em
	white-space normal
	padding-bottom 0.5em

.post2
	margin-left 20px
	margin-bottom 10px
	padding-bottom 20px
	border-bottom 1px solid azul_dark
	width 850px
	display block
	text-align justify
	white-space normal
	overflow hidden

.post2-last
	margin-left 20px
	margin-bottom 10px
	padding-bottom 20px
	width 850px
	display block
	text-align justify
	white-space normal
	overflow hidden
	
.post-nw
	margin-left 20px
	margin-bottom 10px
	padding-bottom 20px
	//width 850px
	display block
	text-align justify
	white-space normal
	overflow hidden

.minipost2
	
	margin-left 20px
	margin-right 1em
	width 400px
	height 250px
	float left
	text-align justify
	white-space normal
	display block
	overflow hidden

.minimagen2
	margin-right 0.5em
	display inline-block
	float left
	img
		border 1px solid azul_darken
		width 150px
		height 150px

.minimagen-alt2
	margin-left 0.5em
	display inline-block
	float right
	img
		border 1px solid azul_darken
		width 150px
		height 150px

.eslogan-text
	color white
	text-shadow 1px 1px 5px azul
	font-size 5em
	white-space nowrap
	display block	

.eslogan-text-alt
	color white
	text-shadow 1px 1px 5px azul
	font-size 5em
	white-space nowrap
	text-align left
	display block	

.form
	border-radius 20px
	padding 3em 0
	width 460px
	height 240px
	background-color azul_light
	display inline-block
	h2
		text-shadow 1px 1px 1px azul_dark
	h4
		text-shadow 1px 1px 1px azul_dark
		color azul_darken
		display inline-block
	input
		font-family 'TrebuchetN'
		font-style italic
		border-radius 5px
		color gris_dark
		background-color white
		display inline-block
	button
		margin-top 50px
		border-radius 20px
		width 120px
		color azul
		background-color azul_lighter
		display inline-block

form
	position relative
	overflow hidden
	.container
		display inline-block
		.con_eslogan
			width 450px
			height 200px
			float left
			h3
				margin-bottom 0.5em
			p
				margin-right 0.25em
				display inline-block
			input
				width 80%
				margin-bottom 0.25em	
		.con_services
			width 450px
			height 200px
			text-align center
			float right
			h3
				margin-bottom 0.25em
